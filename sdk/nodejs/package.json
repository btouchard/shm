{
  "name": "@btouchard/shm-sdk",
  "version": "1.0.0",
  "description": "Privacy-first telemetry SDK for self-hosted software",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "npm run build && node --test dist/**/*.test.js",
    "test:dev": "node --experimental-strip-types --experimental-transform-types --test"
  },
  "keywords": [
    "telemetry",
    "metrics",
    "self-hosted",
    "privacy",
    "analytics"
  ],
  "author": "Benjamin Touchard",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btouchard/shm.git",
    "directory": "sdk/nodejs"
  },
  "homepage": "https://github.com/btouchard/shm/tree/main/sdk/nodejs#readme",
  "bugs": {
    "url": "https://github.com/btouchard/shm/issues"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  }
}
